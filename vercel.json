{
    "version": 2,

    "build":[
        {
            "src": "server.js", 
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/contact",
          "dest": "server.js"
        },
        {
          "src": "/(.*)",
          "dest": "/index.html"
        }
      ]
  }
  
  